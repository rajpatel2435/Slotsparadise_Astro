    <script>
        document.addEventListener("DOMContentLoaded", function () {
            const toggleBtns = document.querySelectorAll(".toggle-btn");
            const tableBodies = document.querySelectorAll(".table-body");
            const searchInputs = document.querySelectorAll(".search-input") as NodeListOf<HTMLInputElement>;
            const showMoreContainers = document.querySelectorAll(".show-more-container");

            tableBodies.forEach((tableBody, index) => {
                const hiddenRows = tableBody.querySelectorAll(".hidden-row");
                const toggleBtn = toggleBtns[index];
                const toggleText = toggleBtn.querySelector("p");
                const searchInput = searchInputs[index];
                const tableRows = tableBody.querySelectorAll("tr");
                const showMoreContainer = showMoreContainers[index];
                let isExpanded = false;

                function updateTable() {
                if (isExpanded) {
                    // Reset to initial state
                    hiddenRows.forEach((row) => row.classList.add("hidden-row"));
                    tableBody.classList.remove("table-body-full");
                    tableBody.classList.add("table-body-initial");
                    toggleText.textContent = "SHOW FULL TABLE";
                    showMoreContainer.classList.remove("mt-5");
                } else {
                    // Show all rows and update height
                    hiddenRows.forEach((row) => row.classList.remove("hidden-row"));
                    tableBody.classList.remove("table-body-initial");
                    tableBody.classList.add("table-body-full");
                    toggleText.textContent = "SHOW LESS TABLE";
                    showMoreContainer.classList.add("mt-5");
                }
                isExpanded = !isExpanded;
                }

                function filterTable() {
                const filter = searchInput.value.toLowerCase();
                tableRows.forEach((row) => {
                    const cells = row.querySelectorAll("td");
                    let match = false;
                    cells.forEach((cell) => {
                    if (cell.textContent.toLowerCase().includes(filter)) {
                        match = true;
                    }
                    });
                    row.style.display = match ? "" : "none";
                });
                }

                toggleBtn.addEventListener("click", updateTable);
                searchInput.addEventListener("input", filterTable);

                // Initially set the class for the table body
                tableBody.classList.add("table-body-initial");
                toggleText.textContent = "SHOW FULL TABLE";
            });

            /**
             * 
             * mobile menu toggle 
             * 
             * */

            // Function to toggle the dropdown menu
            function toggleDropdown(event) {
                event.preventDefault();  // Prevent default anchor behavior
                
                // Ensure we always get the <a> element even if an image or span is clicked
                const button = event.currentTarget;
                
                const dropdownMenu = button.nextElementSibling;  // Get the dropdown menu
                
                if (dropdownMenu.classList.contains('show')) {
                    // If the dropdown is already shown, hide it
                    dropdownMenu.classList.remove('show');
                    setTimeout(() => dropdownMenu.style.display = 'none', 300);  // Match this with transition duration
                } else {
                    // Otherwise, show the dropdown
                    dropdownMenu.style.display = 'block';
                    setTimeout(() => dropdownMenu.classList.add('show'), 0);
                }
            }

            // Attach event listeners to all share buttons
            document.querySelectorAll('.mob_share_btn').forEach(btn => {
                btn.addEventListener('click', toggleDropdown);
            });


            /**
             * 
             * Funtion to copy the url of the page
             * 
             */


            
            const copyLinkButtons = document.querySelectorAll('.copy-link-btn') as NodeListOf<HTMLAnchorElement>;

            copyLinkButtons.forEach((button) => {
                button.addEventListener('click', (event: MouseEvent) => {
                    event.preventDefault(); // Prevent default link behavior

                    const pageUrl: string = window.location.href;

                    // Copy the page URL to the clipboard
                    navigator.clipboard.writeText(pageUrl).then(() => {
                        showTooltip(button, 'Link copied');
                    }).catch(err => {
                        console.error('Failed to copy: ', err);
                    });
                });
            });


            function showTooltip(element: HTMLElement, message: string) {
                // Create the tooltip element
                const tooltip = document.createElement('div');
                tooltip.innerText = message;
                tooltip.style.position = 'absolute';
                tooltip.style.backgroundColor = 'black';
                tooltip.style.color = 'white';
                tooltip.style.padding = '5px 10px';
                tooltip.style.borderRadius = '4px';
                tooltip.style.fontSize = '12px';
                tooltip.style.zIndex = '1000';
                tooltip.style.whiteSpace = 'nowrap';

                // Append the tooltip to the body to ensure proper positioning
                document.body.appendChild(tooltip);

                // Position the tooltip above the element
                const rect = element.getBoundingClientRect();
                tooltip.style.top = `${rect.top - 30}px`;
                tooltip.style.left = `${rect.left + rect.width / 2 - tooltip.clientWidth / 2}px`;

                // Adjust the position after the element is added to the DOM
                tooltip.style.left = `${rect.left + rect.width / 2 - tooltip.clientWidth / 2}px`;

                // Remove the tooltip after 2 seconds
                setTimeout(() => {
                    tooltip.remove();
                }, 2000);
            }

            
            
            /** 
             * 
             * Table sorter function
             * 
             */

        
            const tables = document.querySelectorAll('.super_table') as NodeListOf<HTMLTableElement>;

            tables.forEach((table) => {
                const headers = table.querySelectorAll('thead td') as NodeListOf<HTMLTableCellElement>;
                
                headers.forEach((header, index) => {
                header.addEventListener('click', () => {
                    sortTable(table, index);
                });
                });
            });


            function sortTable(table: HTMLTableElement, columnIndex: number) {
                const tbody = table.querySelector('tbody');
                if (!tbody) return;  // Safety check

                const rows = Array.from(tbody.querySelectorAll('tr'));
                const isAscending = table.getAttribute('data-sort-asc') === 'true';

                rows.sort((rowA, rowB) => {
                    const cellA = rowA.children[columnIndex] as HTMLTableCellElement;
                    const cellB = rowB.children[columnIndex] as HTMLTableCellElement;

                    const valueA = cellA.innerText.toLowerCase();
                    const valueB = cellB.innerText.toLowerCase();

                    if (valueA < valueB) return isAscending ? -1 : 1;
                    if (valueA > valueB) return isAscending ? 1 : -1;
                    return 0;
                });

                table.setAttribute('data-sort-asc', String(!isAscending));

                rows.forEach(row => tbody.appendChild(row));
            }



        });

  

    </script>
